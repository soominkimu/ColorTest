{
  "name": "color-organizer",
  "version": "5.0.0",
  "description": "A sample React application",
  "main": "index.js",
  "author": "Simon Kay",
  "license": "MIT",
  "scripts": {
    "build": "./node_modules/.bin/webpack --progress",
    "prestart": "npm run build",
    "start": "./node_modules/.bin/babel-node ./src/server"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-jest": "^21.0.2",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "cssnano": "^3.10.0",
    "deep-freeze": "^0.0.1",
    "enzyme": "^2.9.1",
    "enzyme-to-json": "^1.6.0",
    "eslint": "^4.7.1",
    "eslint-plugin-react": "^7.3.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "jest": "^21.1.0",
    "jest-css-modules": "^1.1.0",
    "jest-enzyme": "^3.8.2",
    "optimize-css-assets-webpack-plugin": "^3.2.0",
    "prop-types": "^15.5.10",
    "react-icons": "^2.2.5",
    "react-test-renderer": "^15.6.1"
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "body-parser": "^1.18.1",
    "css-loader": "^0.28.7",
    "express": "^4.15.4",
    "fs": "^0.0.1-security",
    "httpster": "^1.0.3",
    "ignore-styles": "^5.0.1",
    "isomorphic-fetch": "^2.2.1",
    "node-sass": "^4.5.3",
    "path": "^0.12.7",
    "postcss-loader": "^2.0.6",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "uuid": "^3.1.0",
    "webpack": "^3.5.6"
  },
  "jest": {
    "setupFiles": [
      "./__tests__/global.js"
    ],
    "modulePathIgnorePatterns": [
      "global.js"
    ],
    "verbose": true,
    "collectCoverage": true,
    "notify": true,
    "collectCoverageFrom": [
      "src/**"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 50,
        "functions": 50,
        "lines": 50,
        "statements": 50
      }
    },
    "moduleNameMapper": {
      "\\.(scss)$": "<rootDir>/node_modules/jest-css-modules"
    }
  }
}
